
{
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "env": {
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "endOfLine": "auto"
            }
        ],
        "no-console": "off"
    },
    "overrides": [{ "files": ["node_modules/"] }]
}
